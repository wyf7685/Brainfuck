# 函数: 输出 $0 数值
>[-]-<  # $1=-1, 留用
[->>+>+>+<<<<]>>>>[-<<<<+>>>>]<<<<  # 复制$0到$2$3 : $4
>>>>+<[[-]>-<]<<<  # $4=!$3, $3=0
>>>>[  # if $4
  [-]<<<<  # $4=0
  >> {bf/put_int.bf} <<  # 输出0
>>>>] <<<<  # 判断复位$3

>>[  # while $+0(余数)
  # $+0被除数
  > >++[<+++++>-]< <  # $+1=除数10
  {bf/divide.bf}  # $+0 / $+1
  >[-]>[-<+>]<<  # 处理返回值
  # $+0余数, $+1商
>]  # $++ 以商为余继续计算
<+[-{bf/put_int.bf}<+]<  # 倒序输出余数